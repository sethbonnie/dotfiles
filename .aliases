# Git
alias gs='git status'
alias ga='git add'
alias gc='git commit'

# Docker
alias dc='docker-compose'

# Make destructive commands interactive
alias mv='mv -i'
alias cp='cp -i'
alias rm='rm -i'

# Create intermediary directories by default
alias mkdir='mkdir -p'

# Human readable defaults
alias df='df -H'
alias du='du -H'
