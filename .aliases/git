function g {
  case $1 in
  a)
    git add ${@:2};;
  b)
    git branch ${@:2};;
  co)
    git checkout ${@:2};;
  ci)
    git commit ${@:2};;
  cl)
    git clone ${@:2};;
  d)
    git diff ${@:2};;
  pu)
    git pull ${@:2};;
  ps)
    git push ${@:2};;
  rb)
    git rebase ${@:2};;
  s)
    git status ${@:2};;
  esac
}
