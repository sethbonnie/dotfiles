[user]
  name = Seth Bonnie
  email = sethkbonnie@gmail.com
[push]
  default = simple
[color]
  ui = auto
[alias]
  a = add
  s = status
  b = branch
  f = fetch
  d = diff
  i = init
  cl = clone
  ci = commit --verbose
  amend = commit --amend
  co = checkout
  dc = diff --cached
  dh1 = diff HEAD~1
  ds = diff --stat=160,120
  aa = add --all
  ff = merge --ff-only
  noff = merge --no-ff
  fa = fetch --all
  po = push -u origin HEAD
  rb = rebase
  rbi = rebase --interactive
  rbc = rebase --continue
  rbs = rebase --skip
  rba = rebase --abort
  cp = cherry-pick

[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
