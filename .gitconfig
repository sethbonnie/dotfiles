[user]
    name = Seth Bonnie
    email = sethkbonnie@gmail.com
  [push]
  	default = simple
[color]
    ui = auto
[alias]
    s = status
    b = branch
    ci = commit --verbose
    amend = commit --amend
    co = checkout
    di = diff
    dc = diff --cached
    dh1 = diff HEAD~1
    ds = diff --stat=160,120
    aa = add --all
    ff = merge --ff-only
    noff = merge --no-ff
    fa = fetch --all
    po = push origin
